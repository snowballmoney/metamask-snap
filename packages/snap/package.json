{
  "name": "@snowballmoney/metamask-snap",
  "version": "0.2.1",
  "description": "Modular Naming Service Snap is a fantastic addition to the MetaMask wallet, making it super easy to connect with MNS identities. With this feature, you can effortlessly send and receive assets using just an MNS identity name, streamlining your transactions!",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowballmoney/metamask-snap.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "main": "./dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "prepublishOnly": "mm-snap manifest",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest"
  },
  "dependencies": {
    "@metamask/snaps-sdk": "10.1.0",
    "@snowballmoney/chain-agnostic-utils": "latest",
    "@snowballmoney/mns-sdk": "latest"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@metamask/auto-changelog": "^3.4.4",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "6.6.1",
    "@metamask/snaps-jest": "8.9.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jsdoc": "^48.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "prettier-plugin-packagejson": "^2.4.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
